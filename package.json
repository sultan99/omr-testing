{
  "name": "arc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node index",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "lint": "eslint src src private webpack.config.js",
    "storybook": "start-storybook -p 9001 -c private/storybook",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:dev -- webpack"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-split-vendor": "^0.2.0",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "history": "^4.6.1",
    "koa": "^2.5.1",
    "koa-static": "^4.0.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-modal": "^3.1.11",
    "react-router-dom": "^4.1.1",
    "styled-components": "^3.0.2",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.4.0"
